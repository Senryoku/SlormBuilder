{
	"name": "slormbuilder",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"serve-prod": "vue-cli-service serve --mode=production",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"@meforma/vue-toaster": "^1.2.2",
		"core-js": "^3.6.5",
		"fuzzysort": "^1.2.1",
		"vue": "^3.0.0",
		"vue-router": "^4.0.14",
		"vuedraggable": "^4.1.0"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~5.0.3",
		"@vue/cli-plugin-eslint": "~5.0.3",
		"@vue/cli-service": "~5.0.1",
		"@vue/compiler-sfc": "^3.0.0",
		"babel-eslint": "^10.1.0",
		"eslint": "^8.11.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-vue": "^8.5.0",
		"image-webpack-loader": "^8.1.0"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/vue3-essential",
			"eslint:recommended",
			"prettier"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {
			"no-mixed-spaces-and-tabs": [
				"warn",
				"smart-tabs"
			]
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	]
}
